{
  "name": "market",
  "version": "0.0.1-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=market --configuration=es5",
    "start:remote": "ng serve --project=market --configuration=remote",
    "build:dev": "ng build --project=market --configuration=dev",
    "build:stage": "ng build --project=market --configuration=stage",
    "build:prod": "ng build --project=market --configuration=production",
    "build": "npm run build:dev && npm run build:stage && npm run build:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --project=market --configuration=dev",
    "e2e:stage": "ng e2e --project=market --configuration=stage",
    "e2e:prod": "ng e2e --project=market --configuration=production",
    "e2e:denis": "ng e2e --project=market --configuration=denis",
    "e2e:remote": "ng e2e --project=market --configuration=remote",
    "docker:build:dev": "npm run docker:image:build && npm run docker:container:build:dev && npm run docker:container:copy-to-dist && npm run docker:container:remove",
    "docker:build:stage": "npm run docker:image:build && npm run docker:container:build:stage && npm run docker:container:copy-to-dist && npm run docker:container:remove",
    "docker:build:prod": "npm run docker:image:build && npm run docker:container:build:prod && npm run docker:container:copy-to-dist && npm run docker:container:remove",
    "docker:build": "npm run docker:image:build && npm run docker:container:build && npm run docker:container:copy-to-dist && npm run docker:container:remove",
    "docker:image:build": "docker-compose build --no-cache app",
    "docker:container:build:dev": "docker run --name market_app_container market_app npm run build:dev",
    "docker:container:build:stage": "docker run --name market_app_container market_app npm run build:stage",
    "docker:container:build:prod": "docker run --name market_app_container market_app npm run build:prod",
    "docker:container:build": "docker run --name market_app_container market_app npm run build",
    "docker:container:copy-to-dist": "docker cp market_app_container:/usr/src/app/dist/. ./dist",
    "docker:container:remove": "docker container rm market_app_container --force"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@ngneat/until-destroy": "^7.1.6",
    "@types/puppeteer": "^3.0.1",
    "classlist.js": "^1.1.20150312",
    "css-vars-ponyfill": "^2.3.2",
    "moment": "^2.24.0",
    "ng-zorro-antd": "^9.3.0",
    "ngx-cookie-service": "^3.0.0",
    "ngx-infinite-scroll": "^9.0.0",
    "ngx-mask": "9.1.0",
    "ngx-webstorage-service": "^4.1.0",
    "normalize.css": "^8.0.1",
    "puppeteer": "^5.2.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  }
}
