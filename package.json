  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng s market -c=es5",
    "start:remote": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng s market -c=remote",
    "build:dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng b market -c=dev",
    "build:stage": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng b market -c=stage",
    "build:prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng b market -c=production",
    "build": "npm run build:dev && npm run build:stage && npm run build:prod",
    "postbuild:dev": "shx cp -R robots-not-prod.txt dist/market/dev/robots.txt && node optimize-css.js dist/market/dev",
    "postbuild:stage": "shx cp -R robots-not-prod.txt dist/market/stage/robots.txt && node optimize-css.js dist/market/stage",
    "postbuild:prod": "shx cp -R robots.txt dist/market/prod && node optimize-css.js dist/market/prod",
    "test": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng t",
    "lint": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng l",
    "e2e": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng e market",
    "e2e:dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng e market -c=dev",
    "e2e:stage": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng e market -c=stage",
    "e2e:denis": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng e market -c=denis",
    "e2e:ruslan": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng e market -c=ruslan",
    "analyze:stage": "webpack-bundle-analyzer dist/market/stage/stats.json",
    "analyze:prod": "webpack-bundle-analyzer dist/market/prod/stats.json"
  },
  "private": true,
