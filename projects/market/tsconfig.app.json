{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "#environments/*": [
        "projects/market/src/environments/*"
      ],
      "#shared/*": [
        "projects/market/src/app/shared/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "src/environments/*.ts",
    "src/**/index.ts",
  ],
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
