{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/extra-webpack.config.ts",
    "src/index-html-transform.ts",
    "src/test.ts",
    "src/**/*.spec.ts",
    "src/environments/*.ts",
    "src/**/index.ts",
    "src/app/workers/*.worker.ts"
  ],
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
